						var T = new Trie();

						for( var i = 0; i < champion_slugs.length; i++ ) {
							T.insert(champion_slugs[i].toLowerCase());
						}

						$('#search_input_box').bind('blur', function() {
							setTimeout(function(){
								$('#search_suggestions').hide();
							}, 200);
						});

						$('#search_input_box').bind('focus', function(e){
							if( $('#search_suggestions').children().length > 0 ) {
								$('#search_suggestions').fadeIn();
							}
						});

						$('#search_input_box').bind('keyup', function(e){
							if( e.keyCode && e.keyCode == 27 ) {
								$(this).blur();
								return;
							}

							var str = $("<div/>").text($(this).val()).html();
							var slug = str.toLowerCase().replace(/[^a-z]/g, "");
							var champion_slug = null;

							var rows = [];

							if( slug.length ) {

								rows.push('<a href="/search?name=' + str + '"><div>Find summoners named "<strong>' + str + '</strong>"</div></a>');

								var trie_matches = T.autoComplete(slug);

								if( trie_matches.length ) {

									champion_slug = trie_matches.sort(function (a, b) { return a.length - b.length; })[0];

								} else {

									var closest_champion;
									var closest_champion_score = 10;

									$.each(champion_slugs, function(i, o) {
										var score = levenshteinenator(slug, o);
										if( score < closest_champion_score ) {
											closest_champion_score = score;
											closest_champion = o;
										}

									});

									if( closest_champion_score <= 3 ) {
										champion_slug = closest_champion;
									}

								}

								if( champion_slug && all_champions[champion_slug] ) {

									var html = [
										'<div class="suggestive_search_separator"></div>',
										'<div class="suggestive_search_champion_item">',
											'<div class="suggestive_search_champion_icon" style="background: url(//lkimg.zamimg.com/shared/riot/images/champions/' + all_champions[champion_slug].champion_id + '_64.png) center center #000000;"></div>',
											'<div style="display: inline-block; vertical-align: top;">',
												'<div class="suggestive_search_champion_name">' + all_champions[champion_slug].name + '</div>',
												'<div class="suggestive_search_champion_item_nav">',
													'<a href="/guides/list.php?champion=' + all_champions[champion_slug].file + '"><div class="suggestive_search_champion_item_nav_icon" style="background: url(//lkimg.zamimg.com/assets/000/000/296.png) center center #000000;"></div>Guides</a>',
													'<a href="/champions/' + all_champions[champion_slug].file + '"><div class="suggestive_search_champion_item_nav_icon" style="background: url(//lkimg.zamimg.com/assets/000/000/295.png) center center #000000;"></div>Stats</a>',
													'<a href="/models/?champion=' + all_champions[champion_slug].champion_id + '&skin=0"><div class="suggestive_search_champion_item_nav_icon" style="background: url(//lkimg.zamimg.com/assets/000/000/294.png) center center #000000;"></div>Models</a>',
												'</div>',
											'</div>',
										'</div>'
									].join('');

									rows.push(html);

								}

							}

							$('#search_suggestions').empty().html(rows.join(''));

							if( $('#search_suggestions').children().length > 0 ) {
								$('#search_suggestions').fadeIn();
							} else {
								$('#search_suggestions').hide();
							}

						});


						$('#search_input_box').focus(function(){
						
							$('#hide_regions_btn').tipsy("show");
						
						});
						
						$('#search_input_box').blur(function(){
						
							$('#hide_regions_btn').tipsy("hide");
						
						});				
						
						//$('#hide_regions_btn').tipsy({fallback:"Click here to show and hide regions!", gravity: 'nw', fade: true, offset: -4});
						
						$('#search-settings-save-btn').live('click', function(e){

							e.preventDefault();
							
							var selected_regions = $('#fancybox-content [name="enabled-search-region"]:checked').map(function(i, el){
							
								return $(this).val();
							
							}).get();
							
							if( selected_regions.length > 0 ) {
							
								LOLKING.createCookie('enabled-search-regions', selected_regions.join(','), 60*60*24*365);
								window.location.reload(true);
							
							} else {
							
								$('#fancybox-content .notification').css('color', '#F44').text('Error: Please select at least 1 region.');
							
							}

						});
												$('#hide_regions_btn').click(function(e){

							e.preventDefault();
							var search_content1 = [
							
								'<div style="overflow: hidden; background: none repeat scroll 0% 0% transparent; padding: 0px;">',
								'	<div class="fancy-modal" style="width: 400px;">',
