    var ads = new function() {
        var scriptCalled = false;
        var queue = [];

        this.load = function(id, channelId) {
            queue.push({
                auid: id,
                slot_id: 'slot' + id,
                tid: channelId,
                onAdUnitLoaded: ads.onLoad
            });
            ads.init();
        };

        this.init = function() {
            if (!scriptCalled) {
                scriptCalled = true;
                $.getScript('http://ox-d.zam.com/w/1.0/jstag');
            }
            if (typeof OX == 'undefined') {
                setTimeout(ads.init, 50);
                return;
            }
            window.ox_instances = window.ox_instances || {};
            var ad, ad_i = window['ox_instances'], inst;
            while((ad = queue.shift())){
                inst = ad_i[ad.auid] = OX(ad);
                inst.load();
            }
        };

        this.onLoad = function(unit) {
            var r_advs = {'':1,119398:1,118043:1},
                ns = 'ox_instances',
                inst = window[ns] && window[ns][unit.get('adunit_id')] || null,
                adv_id = unit.get('adv_id'),
                adv_id = adv_id.substr(adv_id.indexOf(':') + 1);
                
            if ( !( adv_id in r_advs || unit.get('adv_id').indexOf('-') > -1 ) ) {
                if (inst) inst.setRefreshMax(0);
            }
                    };
    };
