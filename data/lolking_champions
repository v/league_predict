								'		<div style="background: #1A1A1A; padding: 10px; margin-bottom: 10px; border-radius: 6px; box-shadow: 0 0 2px #000 inset;">',
								'			<div class="jerek-list-filter champion-list-filter">',
								'				<table>',
								'					<tr id="jerek-list-filter-sort">',
								'						<th>Enabled Regions</th>',
								'						<td>'];

                                                        search_content1.push('<input type="checkbox" name="enabled-search-region" value="na" checked="checked" id="enabled-search-region-na">');
                            search_content1.push('<label for="enabled-search-region-na">North America</label><br />');
                                                        search_content1.push('<input type="checkbox" name="enabled-search-region" value="euw" checked="checked" id="enabled-search-region-euw">');
                            search_content1.push('<label for="enabled-search-region-euw">Europe West</label><br />');
                                                        search_content1.push('<input type="checkbox" name="enabled-search-region" value="eune" checked="checked" id="enabled-search-region-eune">');
                            search_content1.push('<label for="enabled-search-region-eune">Europe Nordic &amp; East</label><br />');
                                                        search_content1.push('<input type="checkbox" name="enabled-search-region" value="br" checked="checked" id="enabled-search-region-br">');
                            search_content1.push('<label for="enabled-search-region-br">Brazil</label><br />');
                                                        search_content1.push('<input type="checkbox" name="enabled-search-region" value="tr" checked="checked" id="enabled-search-region-tr">');
                            search_content1.push('<label for="enabled-search-region-tr">Turkey</label><br />');
                                                        search_content1.push('<input type="checkbox" name="enabled-search-region" value="ru" checked="checked" id="enabled-search-region-ru">');
                            search_content1.push('<label for="enabled-search-region-ru">Russia</label><br />');
                                                        search_content1.push('<input type="checkbox" name="enabled-search-region" value="lan" checked="checked" id="enabled-search-region-lan">');
                            search_content1.push('<label for="enabled-search-region-lan">Latin America North</label><br />');
                                                        search_content1.push('<input type="checkbox" name="enabled-search-region" value="las" checked="checked" id="enabled-search-region-las">');
                            search_content1.push('<label for="enabled-search-region-las">Latin America South</label><br />');
                                                        search_content1.push('<input type="checkbox" name="enabled-search-region" value="oce" checked="checked" id="enabled-search-region-oce">');
                            search_content1.push('<label for="enabled-search-region-oce">Oceania</label><br />');
                            
                            var search_content2 = [
	                            '						</td>',
								'					</tr>',
								'				</table>',
								'			</div>',
								'		</div>',
								'		<div class="notification" style="font-size: 12px; color: #AAA; margin-bottom: 10px; margin-left: 10px;">Note: If you select only 1 region you will be sent directly to the profile if a match is found.</div>',
								'		<div style="text-align: center; clear: both;">',
								'			<button style="margin-right: 5px;" class="green" id="search-settings-save-btn">Save Settings</button>',
								'			<button onclick="$.fancybox.close();" class="grey">Cancel</button>',
								'		</div>',										
								'	</div>',
								'</div>'];

                            var combined_content = search_content1.join("\n") + search_content2.join("\n");

							$.fancybox({ transitionIn: 'none', padding: 0, transitionOut: 'none', speedIn: 0, speedOut: 0, showCloseButton: false, content: combined_content});

						});
					});
